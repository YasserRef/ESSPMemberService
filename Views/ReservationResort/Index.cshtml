@model IEnumerable<ESSPMemberService.V_RESERVATION_RESORT>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">

<h1>عرض كل المصايف</h1>


<form method="get" class="mb-3">
    <div class="form-row">
        <div class="form-group col-md-2">
            <label>فرع</label>
            <input type="number" name="branchNo" class="form-control" value="@Context.Request.Query[" branchNo"]" />
        </div>
        <div class="form-group col-md-2">
            <label>كود المصيف</label>
            <input type="number" name="holidayCode" class="form-control" value="@Context.Request.Query["holidayCode"]" />
        </div>
        <div class="form-group col-md-2">
            <label>رقم الأسبوع</label>
            <input type="number" name="weekNo" class="form-control" value="@Context.Request.Query["weekNo"]" />
        </div>
        <div class="form-group col-md-2">
            <label>رقم الوحدة</label>
            <input type="number" name="flatCode" class="form-control" value="@Context.Request.Query["flatCode"]" />
        </div>
        <div class="form-group col-md-2 align-self-end">
            <button type="submit" class="btn btn-primary">بحث</button>
        </div>
    </div>
</form>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RECIEPT_YEAR)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RECIEPT_DATE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BRANCH_NO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.USER_CODE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.F_MEM)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HOLIDAY_CODE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.HOLIDAY_YEAR)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FLAT_CODE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.WEEK_NO)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FLAT_VALUE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.INSURE_VALUE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RECIEPT_VALUE)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CANCEL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DISCOUNT)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.F_STAMP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RES_PK)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RECIEPT_YEAR)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RECIEPT_DATE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BRANCH_NO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.USER_CODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.F_MEM)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HOLIDAY_CODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HOLIDAY_YEAR)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FLAT_CODE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.WEEK_NO)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FLAT_VALUE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.INSURE_VALUE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RECIEPT_VALUE)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CANCEL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DISCOUNT)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.F_STAMP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RES_PK)
            </td>
            <td>
                <a asp-action="التفاصيل" asp-route-id="@item.FLAT_CODE">Details</a>
            </td>
        </tr>
}
    </tbody>
</table>

</div>
