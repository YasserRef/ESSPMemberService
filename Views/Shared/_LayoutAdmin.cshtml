<!DOCTYPE html>
<html dir="rtl" lang="ar">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link id="Link1" rel="icon" href="ESSP.ico" />
    <title>@ViewData["Title"] - نقابة العلميين</title>

   

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

    <script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>

    <link id="theme-stylesheet" rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/5.1.0/css/bootstrap.min.css"> <!-- Default Theme -->
    <!-- Additional theme links (uncomment to test) -->
  
    <link rel="stylesheet" href="~/css/themes/default/theme.css" />

</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">نقابة العلميين</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="V_T_DIFFMEMBER" asp-action="Index">متاخرات الاعضاء</a>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle text-dark" href="#" id="pensionsMenu" role="button" data-bs-toggle="dropdown" aria-expanded="false">
                                معاشات الاعضاء
                            </a>
                            <ul class="dropdown-menu" aria-labelledby="pensionsMenu">
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="V_SPENDDATE" asp-action="IndexSelf">معاشات احياء</a>
                                </li>
                                <li>
                                    <a class="dropdown-item" asp-area="" asp-controller="V_SPENDDATE" asp-action="Index">معاشات ورثه</a>
                                </li>
                            </ul>
                        </li>

                      
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="TRAINING_DESCRIPTION" asp-action="Index">التدريب</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="BRANCH_DESCRIPTION" asp-action="Index">الفروع</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="V_REQUESTS" asp-action="CreateRequest">تسجيل شكوى</a>
                        </li>
                       @*  <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="PAYMENT_BANK" asp-action="Create">الدفع الالكترونى</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="T_PAYMENT_COMPANY" asp-action="Index">شركات الدفع الالكترونى</a>
                        </li> *@
                    </ul>
                    <partial name="_LoginPartialAdmin" />
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/js/bootstrap.bundle.min.js"></script>
        <script src="https://cdn.jsdelivr.net/npm/popperjs/core@2.9.3/dist/umd/popper.min.js"></script>
        

    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2024 - نقابة العلميين - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>   
   
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
